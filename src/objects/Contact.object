        <formula>if( Fax == &quot;&quot; || HasOptedOutOfFax, Email , substitute(substitute(substitute(substitute(Fax,&quot;-&quot;,&quot;&quot;),&quot;(&quot;,&quot;&quot;),&quot;)&quot;,&quot;&quot;),&quot; &quot;,&quot;&quot;) + &quot;.&quot; + CASESAFEID($Organization.Id) + &quot;.&quot; + CASESAFEID(Id) + &quot;@mfb.zpaper.com&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
