    <design:attribute name="HDOT" label="HDOT" datasource="Yes,No" description="Please select the filter"/>
    <design:attribute name="a3500a" label="3500" datasource="Yes,No" description="Please select the filter"/>
    <design:attribute name="DOSE" label="DOSE" datasource="Yes,No" description="Please select the filter"/>
    <design:attribute name="ENRLC" label="ENRLC" datasource="Yes,No" description="Please select the filter"/>
    <design:attribute name="PIRM" label="PIRM" datasource="Yes,No" description="Please select the filter"/>
    <design:attribute name="REFR" label="REFR" datasource="Yes,No" description="Please select the filter"/>
    
